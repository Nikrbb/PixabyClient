ed\" @change=\"sortJson\" name=\"option\" class=\"sort\" id=\"sort\">\n    <option class=\"sort__item\" value=\"\">-</option>\n    <option class=\"sort__item\"  value=\"Likes\">Likes</option>\n    <option class=\"sort__item\" value=\"Comments\">Comments</option>\n</select>\n<div class=\"filter\">\n    <input v-model=\"inputField\" @keyup=\"debounce(this.inputFilter)\" type=\"text\" placeholder=\"filter\" class=\"filter__tags\">\n</div>\n\n<div  class=\"container\">\n  <div  class=\"card\"  v-for=\"card in cards\" :key=\"card.id\">\n        <a v-bind:href=\"card.pageURL\" target=\"_blank\" class=\"card__image\">\n            <img v-bind:src=\"card.largeImageURL\" alt=\"random-picture\">\n        </a>\n        <div class=\"card__social\">\n            <div class=\"card__social_likes\"><i class=\"likes fas fa-thumbs-up\"></i> {{ card.likes}}</div>\n            <div class=\"card__social_comments\">{{ card.comments}}<i class=\"comments fas fa-comments\"></i></div>\n        </div>\n        <div class=\"card__tags\">\n            <span v-for=\"tag of this.tagsList(card)\" @click=\"this.try\" :key=\"tag\" class=\"card__tags-item\">{{ tag }}</span>\n\n\n    </div>\n  </div>\n</div>\n</template>\n\n\n<script>\n\nexport default {\n    name: 'CatsList',\n        data() {\n            return {\n                selected: '',\n                inputField: '',\n            }\n        },\n    computed: {\n        cards() {\n            return this.$store.getters.getJson;\n        },\n        tags() {\n            return this.$store.getters.getTags;\n        },\n        inputFilter() {\n            console.log(this.inputField);\n            \n        },\n        \n    },\n    methods: {\n\n        try() {\n            console.log(this.tags);\n        },\n\n        debounce(fn) {\n            let timeout;\n            return function () {\n                clearTimeout(timeout);\n                timeout = setTimeout( () => {\n                    fn.apply(this)\n                }, 1000);\n            }\n        },\n\n        tagsList(arg) {\n            let cardTags = [];\n            for (let i of arg.tags.split(',')) {\n                cardTags.push(i.trim());\n            };\n            return cardTags\n        },\n\n        sortJson() {\n            this.$store.dispatch(`loadJson`, this.selected)\n        }\n    },\n    async mounted() {\n \n    }\n}\n</script>\n\n\n"]}]}